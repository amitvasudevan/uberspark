/*
add uobj json manifest
author: amit vasudevan (amitvasudevan@acm.org)
*/

{
	/* header */
	"hdr":{
		"type" : "uobj",
		"namespace" : "/uberspark/uobjs/generic/test/add",
		"platform" : "generic",
		"arch" : "generic",
		"cpu" : "generic"
	},

	/* sources */
	"uobj-sources" : {
		"h-files": [ "typedefs.h", "constdefs.h" ],
		"c-files": [ "main.c" ],
		"casm-files": [ ]
	},
	
	/* public methods */
	"uobj-publicmethods" : {
		"add" : [
			"uint32_t",
			"(uint32_t param1, uint32_t param2)", 
			"2" 
		]
	},

	/* public methods of other uobjs that this uobj invokes */
	"uobj-callees" : {
		"testuobj": ["one", "two", "three"]
	},
	
	/* legacy CoSS functions/code that this uobj invokes */
	"uobj-exitcallees": [
		"untrusted_func_1",
		"untrusted_func_2"
	] 


	,
	"uobj-binary" : {
		"uobj-sections": {
			"uobj_testsect" 		: [ "0x0", "0x111", "0x1000", "0x100000", "0x100000", ".testsect", ".testsect1" ]

		}
	}

	
}



