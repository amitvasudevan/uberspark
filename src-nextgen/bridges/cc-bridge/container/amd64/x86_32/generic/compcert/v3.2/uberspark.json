/*
    uberSpark code bridges json definition
*/
{
	/* manifest header */
	"uberspark-manifest": {
		"namespace": [
			"uberspark-bridge-cc"
		],
		"version_min": "any",
		"version_max": "any"
	},
	"uberspark-bridge-cc": {
		/* bridge header */
		"bridge-hdr": {
			"btype": "container",
			"bname": "compcert",
			"execname": "ccomp",
			"devenv": "amd64",
			"arch": "x86_32",
			"cpu": "generic",
			"version": "v3.2",
			"path": ".",
			"params": [
				"-fpacked-structs",
				"-fstruct-passing",
				"-Wno-c11-extensions"
			],
			"container_fname": "uberspark_bridges.Dockerfile"
		},
		"params_prefix_obj": "-c",
		"params_prefix_asm": "-S",
		"params_prefix_output": "-o",
		"params_prefix_include": "-I",
		"params_cclib": "/usr/local/lib/compcert/libcompcert.a"
	}
}