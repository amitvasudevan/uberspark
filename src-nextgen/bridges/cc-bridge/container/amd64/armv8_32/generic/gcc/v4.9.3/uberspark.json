/*
    uberSpark code bridges json definition
*/

{
		"uberspark.manifest.namespace" : [ "uberspark-bridge-cc" ],
		"uberspark.manifest.version_min" : "any",
		"uberspark.manifest.version_max" : "any",

    "uberspark-bridge-cc" : {
		/* bridge header */
		"bridge-hdr":{
			"category" : "container",
			"name" : "gcc",
			"executable_name" : "arm-linux-gnueabihf-gcc-4.9",
			"dev_environment" : "amd64",
			"arch" : "armv8_32",
			"cpu" : "generic",
			"version" : "v4.9.3",
			"path" : ".",
		    "parameters" : [
			    "-nostdinc",
			    "-nostdlib",
           	    "-nostartfiles",
				"-ffreestanding",
				"-marm",
				"-march=armv8-a",
				"-mno-thumb-interwork"
			],
			"container_filename" : "uberspark_bridges.Dockerfile"
		},

	"params_prefix_obj" : "-c",
        "params_prefix_asm" : "-S",
        "params_prefix_output" : "-o",
		"params_prefix_include" : "-I",
		"params_cclib" : "/usr/lib/gcc-cross/arm-linux-gnueabihf/4.9/libgcc.a"
	}

}
