/*
    uberSpark code bridges json definition
*/

{
   	/* manifest header */
	   "uberspark-manifest":{
		"namespace" : [ "uberspark-bridge-loader" ],
		"version_min" : "any",
		"version_max" : "any"
	},

    "uberspark-bridge-loader" : {
		/* bridge header */
		"bridge-hdr":{
			"category" : "container",
			"name" : "gcc",
			"executable_name" : "gcc",
			"dev_environment" : "amd64",
			"arch" : "x86_32",
			"cpu" : "generic",
			"version" : "v5.5.0",
			"path" : ".",
			"parameters" : [ ],
			"container_filename" : "uberspark_bridges.Dockerfile"
		},

		"bridge_prefix" : "baremetal",
		"bridge_cmd" : [
			"echo Default loader bridge command. Nothing to do!"
		] 

	}

}
