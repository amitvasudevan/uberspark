{

		"uberspark.manifest.namespace" : "uberspark/uobjcoll",
		"uberspark.manifest.version_min" : "5.1",
		"uberspark.manifest.version_max" : "any",

		"uberspark.uobjcoll.namespace" : "uberspark/uobjcoll/generic/hello-mul",
		"uberspark.uobjcoll.platform" : "generic",
		"uberspark.uobjcoll.arch" : "armv8_32",
		"uberspark.uobjcoll.cpu" : "generic",
		"uberspark.uobjcoll.hpl" : "any",
		"uberspark.uobjcoll.sentinels_intra_uobjcoll" : [ "generic/generic/any/call" ],

		/* uobjcoll configuration definitions */
		"uberspark.uobjcoll.configdefs" : [

			{ "name": "example_hexnumber", "value": "0x01000000"},
			{ "name": "example_decnumber", "value": "4096"},
			{ "name": "example_string", "value": "\"this is a string definition\""},
			{ "name": "example_booldef", "value": "@@TRUE@@"}

		],
	
		/* uobjs */
		"uberspark.uobjcoll.uobjs" : {
			"master" : "",
			"templars" : [
				"uberspark/uobjcoll/generic/hello-mul/uobjs/main"
			]
		},

		/* uobjcoll init method */
		"uberspark.uobjcoll.init_method" : {
			"uobj_namespace" : "uberspark/uobjcoll/generic/hello-mul/uobjs/main",
			"public_method" : "init",
			"sentinels" : [
				{
					"sentinel_type" : "baremetal/vccore/srot/generic/rpi3/hyp/call",
					/*"sentinel_type" : "baremetal/grub-legacy/srot/generic/pc/pmr0/call",*/

					"sentinel_size" : "0x10000"	
				}	
			]
		},	


		/* uobjcoll public methods */
		"uberspark.uobjcoll.public_methods" : {
			"uberspark/uobjcoll/generic/hello-mul/main" : {
				"main" : [ "generic/generic/any/call" ]
			}
		}	


}



