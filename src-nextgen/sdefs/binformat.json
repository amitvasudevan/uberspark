{
	/* header */
	"hdr":{
		"type" : "shareddefs",
		"namespace" : "",
		"platform" : "generic",
		"arch" : "generic",
		"cpu" : "generic"
	},


    "binformat_section": 
    {
        "constdef": 
        {
            "USBINFORMAT_SECTION_TYPE_PADDING": ["int", "0x0"],
            "USBINFORMAT_SECTION_TYPE_UOBJ": ["int", "0x1"],
            "USBINFORMAT_SECTION_TYPE_UOBJCOLL_ENTRYSENTINEL": ["int", "0x2"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_RESUMESENTINEL": ["int", "0x3"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_CALLEESENTINEL": ["int", "0x4"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_EXITCALLEESENTINEL": ["int", "0x5"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_HDR": ["int", "0x6"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_CODE": ["int", "0x7"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_RWDATA": ["int", "0x8"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_RODATA": ["int", "0x9"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_USTACK": ["int", "0xa"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_TSTACK": ["int", "0xb"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_USTACKTOS": ["int", "0xc"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_TSTACKTOS": ["int", "0xd"],
            "USBINFORMAT_SECTION_TYPE_UOBJ_DMADATA": ["int", "0xe"]
         }
    }

}