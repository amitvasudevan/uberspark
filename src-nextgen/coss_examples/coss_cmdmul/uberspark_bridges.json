/*
    uberSpark code bridges json definition
*/

{
	/* uberspark header */
	"uberspark-hdr":{
		"coss_version" : "0.1",
		"mftype" : "bridge",
		"uberspark_min_version" : "any",
		"uberspark_max_version" : "any"
	},

    "bridge-cc" : {
        /* bridge header */
        "bridge-hdr":{
            "btype" : "container",
            "execname" : "gcc",
            "devenv" : "amd64",
            "arch" : "x86_32",
            "cpu" : "generic",
            "version" : "v5.4.0",
            "path" : ".",
            "params" : [ "-v" ],
            "container_fname" : "uberspark_bridges.Dockerfile"
        },

        "params_prefix_to_obj" : "-c",
        "params_prefix_to_asm" : "-S",
        "params_prefix_to_output" : "-o"
    }

}